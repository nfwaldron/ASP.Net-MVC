@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<p>Use this area to provide additional information.</p>

@* In order to allow logged in users to see particular parts of their website *@

@if (this.User.Identity.IsAuthenticated)
{
    <div>
        Welcome @User.Identity.Name. Supreme Ruler
    </div>
}
else 
{
    <div>
        Go Away Ananymous Person!
    </div>
}